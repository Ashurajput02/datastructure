#include<stdio.h>
int front=-1;
int rear=-1;

int n=5;
int main(){

int arr[n];
    enqueue(4,arr);
    enqueue(5,arr);
    enqueue(6,arr);
    enqueue(7,arr);
    enqueue(8,arr);
display(arr);
dequeue();
display(arr);
dequeue();
dequeue();
dequeue();
enqueue(2,arr);
display(arr);

    enqueue(9,arr);
    display(arr);
}

int isempty(){
    if (front==-1 &&rear==-1)
    return 1;
    else 
    return 0;
}
int isfull(){
    if(((rear+1)%n)==front)
    return 1;
    else
    return 0;

}
void enqueue(int x,int arr[5]){
    if(isfull()==1){
        printf("queue overflow error\n");
    return;
    }
if(isempty()){
    front=(front+1)%n;
}
rear=(rear+1)%n;
arr[rear]=x;
}

void dequeue(){
if(isempty()){
printf("nothing to remove\n");
return;}
if(front==rear){
    front=-1;
    rear=-1;
}
front=(front+1)%n;

}

void display(int arr[5]) {
    if (front == -1) {
        printf("Queue is empty.\n");
        return;
    }

    printf("Elements in the queue: ");
    if (rear >= front) {
        for (int i = front; i <= rear; i++) {
            printf("%d ", arr[i]);
        }
    } else {
        for (int i = front; i < n; i++) {
            printf("%d ", arr[i]);
        }
        for (int i = 0; i <= rear; i++) {
            printf("%d ", arr[i]);
        }
    }
    printf("\n");
}
/*display function main yadi rear<front se tab pehle front se leke n tak elements print kara lo aur uske baad 0 se leke rear tak elements print kara lo; */


